{
  "name": "saaschat-app",
  "version": "0.0.1",
  "scripts": {
    "sveltekit:dev": "vite dev --host",
    "dev": "bun run sveltekit:dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "build:widget": "vite -c vite.widget.config.js build"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@iconify/svelte": "^3.1.6",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.30.4",
    "@types/cookie": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.43.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^5.0.0-next.201",
    "svelte-check": "^3.8.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^4.5.3"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "@tanstack/svelte-query": "^5.51.15",
    "@tanstack/svelte-query-devtools": "^5.51.15",
    "@tiptap/core": "^2.5.7",
    "@tiptap/pm": "^2.5.7",
    "@tiptap/starter-kit": "^2.5.7",
    "autoprefixer": "^10.4.19",
    "date-fns": "^3.6.0",
    "postcss": "^8.4.40",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.7"
  }
}
